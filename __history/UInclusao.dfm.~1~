  object FDQuery2: TFDQuery
    Connection = DM.FDConnection1
    SQL.Strings = (
      'select Vitoria, '#39'A'#39' as Equipe,'
      
        'a1ME.Descricao as A1MENOR, a2ME.Descricao as A2MENOR, a3ME.Descr' +
        'icao as A3MENOR,'
      
        'b1ME.Descricao as B1MENOR, b2ME.Descricao as B2MENOR, b3ME.Descr' +
        'icao as B3MENOR'
      'from LOTECA_2019'
      
        '            join ARCANOS_MENORES a1ME on(a1ME.Numero = A_CASA1_M' +
        'ENOR and a1ME.Naipe = A_CASA1_NAIPE)'
      
        '            join ARCANOS_MENORES a2ME on(a2ME.Numero = A_CASA2_M' +
        'ENOR and a2ME.Naipe = A_CASA2_NAIPE)'
      
        '            join ARCANOS_MENORES a3ME on(a3ME.Numero = A_CASA3_M' +
        'ENOR and a3ME.Naipe = A_CASA3_NAIPE)'
      
        '            join ARCANOS_MENORES b1ME on(b1ME.Numero = B_CASA1_M' +
        'ENOR and b1ME.Naipe = B_CASA1_NAIPE)'
      
        '            join ARCANOS_MENORES b2ME on(b2ME.Numero = B_CASA2_M' +
        'ENOR and b2ME.Naipe = B_CASA2_NAIPE)'
      
        '            join ARCANOS_MENORES b3ME on(b3ME.Numero = B_CASA3_M' +
        'ENOR and b3ME.Naipe = B_CASA3_NAIPE)'
      'WHERE Vitoria is not null'
      
        'and ((A_CASA1_MENOR = :lsArcanoMenor1 and A_CASA1_NAIPE = :lsNai' +
        'pe1 and'
      
        '      A_CASA2_MENOR = :lsArcanoMenor2 and A_CASA3_NAIPE = :lsNai' +
        'pe3)'
      '     or'
      
        '     (A_CASA1_MENOR = :lsArcanoMenor1 and A_CASA1_NAIPE = :lsNai' +
        'pe1 and'
      
        '      A_CASA2_NAIPE = :lsNaipe2       and A_CASA3_MENOR = :lsArc' +
        'anoMenor3)'
      '     or'
      '     (A_CASA1_MENOR = :lsArcanoMenor1 and'
      
        '      A_CASA2_MENOR = :lsArcanoMenor2 and A_CASA2_NAIPE = :lsNai' +
        'pe2 and'
      '      A_CASA3_NAIPE = :lsNaipe3)'
      '     or'
      '     (A_CASA1_NAIPE = :lsNaipe1 and'
      
        '      A_CASA2_MENOR = :lsArcanoMenor2 and A_CASA2_NAIPE = :lsNai' +
        'pe2 and'
      '      A_CASA3_MENOR = :lsArcanoMenor3)'
      '     or'
      
        '     (A_CASA1_MENOR = :lsArcanoMenor1 and A_CASA2_NAIPE = :lsNai' +
        'pe2 and'
      
        '      A_CASA3_MENOR = :lsArcanoMenor3 and A_CASA3_NAIPE = :lsNai' +
        'pe3)'
      '     or'
      
        '     (A_CASA1_NAIPE = :lsNaipe1       and A_CASA2_MENOR = :lsArc' +
        'anoMenor2 and'
      
        '      A_CASA3_MENOR = :lsArcanoMenor3 and A_CASA3_NAIPE = :lsNai' +
        'pe3))'
      'UNION'
      'select Vitoria, '#39'B'#39' as Equipe, '
      
        'a1ME.Descricao as A1MENOR, a2ME.Descricao as A2MENOR, a3ME.Descr' +
        'icao as A3MENOR,'
      
        'b1ME.Descricao as B1MENOR, b2ME.Descricao as B2MENOR, b3ME.Descr' +
        'icao as B3MENOR'
      'from LOTECA_2019 '
      
        '            join ARCANOS_MENORES a1ME on(a1ME.Numero = A_CASA1_M' +
        'ENOR and a1ME.Naipe = A_CASA1_NAIPE)'
      
        '            join ARCANOS_MENORES a2ME on(a2ME.Numero = A_CASA2_M' +
        'ENOR and a2ME.Naipe = A_CASA2_NAIPE)'
      
        '            join ARCANOS_MENORES a3ME on(a3ME.Numero = A_CASA3_M' +
        'ENOR and a3ME.Naipe = A_CASA3_NAIPE)'
      
        '            join ARCANOS_MENORES b1ME on(b1ME.Numero = B_CASA1_M' +
        'ENOR and b1ME.Naipe = B_CASA1_NAIPE) '
      
        '            join ARCANOS_MENORES b2ME on(b2ME.Numero = B_CASA2_M' +
        'ENOR and b2ME.Naipe = B_CASA2_NAIPE)'
      
        '            join ARCANOS_MENORES b3ME on(b3ME.Numero = B_CASA3_M' +
        'ENOR and b3ME.Naipe = B_CASA3_NAIPE) '
      'WHERE Vitoria is not null '
      
        'and ((B_CASA1_MENOR = :lsArcanoMenor1 and B_CASA1_NAIPE = :lsNai' +
        'pe1 and'
      
        '      B_CASA2_MENOR = :lsArcanoMenor2 and B_CASA3_NAIPE = :lsNai' +
        'pe3)'
      '     or'
      
        '     (B_CASA1_MENOR = :lsArcanoMenor1 and B_CASA1_NAIPE = :lsNai' +
        'pe1 and'
      
        '      B_CASA2_NAIPE = :lsNaipe2       and B_CASA3_MENOR = :lsArc' +
        'anoMenor3)'
      '     or'
      '     (B_CASA1_MENOR = :lsArcanoMenor1 and'
      
        '      B_CASA2_MENOR = :lsArcanoMenor2 and B_CASA2_NAIPE = :lsNai' +
        'pe2 and'
      '      B_CASA3_NAIPE = :lsNaipe3)'
      '     or'
      '     (B_CASA1_NAIPE = :lsNaipe1 and'
      
        '      B_CASA2_MENOR = :lsArcanoMenor2 and B_CASA2_NAIPE = :lsNai' +
        'pe2 and'
      '      B_CASA3_MENOR = :lsArcanoMenor3)'
      '     or'
      
        '     (B_CASA1_MENOR = :lsArcanoMenor1 and B_CASA2_NAIPE = :lsNai' +
        'pe2 and'
      
        '      B_CASA3_MENOR = :lsArcanoMenor3 and B_CASA3_NAIPE = :lsNai' +
        'pe3)'
      '     or'
      
